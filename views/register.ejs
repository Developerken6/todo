<% include ./partials/header %>

<div class="container-fluid">
  <div class="row">
    <div class="col">
      <form action="/register" method="POST">
        <h1>Register Form</h1>
        <div class="form-group">
          <label>Enter a username here:</label>
          <input class="form-control" type="text" name="username" placeholder="enter a new username here">
        </div>
        <div class="form-group">
          <label>Enter a password here:</label>
          <input class="form-control" type="password" name="password" placeholder="enter a new password here">
        </div>
        <div class="form-group">
          <button id="btn" class="btn btn-primary" type="submit" name="button">Submit</button>
        </div>
        </form>

    </div>
  </div>
</div>
<% include ./partials/footer%>
